on chat:
	if player is op:
		stop
	else:
		replace all "fasz" in message with "***"
		replace all "pénisz" in message with "***"
		replace all "kurva" in message with "***"
		replace all "ribanc" in message with "***"
		replace all "anyád" in message with "***"
